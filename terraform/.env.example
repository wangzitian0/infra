# Copy to terraform/.env, fill values, then load with:
#   set -a; source terraform/.env; set +a
# After loading, run: cd terraform && terraform init && terraform plan

TF_VAR_vps_host=1.2.3.4
TF_VAR_vps_user=root
TF_VAR_ssh_port=22
TF_VAR_ssh_private_key="-----BEGIN OPENSSH PRIVATE KEY-----\nPASTE_KEY_WITH_ESCAPED_NEWLINES\n-----END OPENSSH PRIVATE KEY-----"
TF_VAR_api_endpoint=
TF_VAR_cluster_name=truealpha-k3s
TF_VAR_k3s_channel=stable
TF_VAR_k3s_version=
TF_VAR_disable_components='["traefik"]'

# Cloudflare R2 state (必填，S3-compatible, no locking)
AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
